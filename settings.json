{
    "vim.insertModeKeyBindings": [
      {
        "before": ["<S-l>"],
        "commands": [":tabnext"]
      },
      {
        "before": ["<S-h>"],
        "commands": [":tabprev"]
      },
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      },
      {
        "before": ["K"],
        "commands": ["lineBreakInsert"],
        "silent": true
      },
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<S-l>"],
        "commands": [":tabnext"]
      },
      {
        "before": ["<S-h>"],
        "commands": [":tabprev"]
      },
      {
        "before": ["<C-h>"],
        "commands": [":tabm -"]
      },
      {
        "before": ["<C-l>"],
        "commands": [":tabm +"]
      },
      { // Go to Definition
        "before": ["<leader>", "g", "d"],
        "commands": ["editor.action.revealDefinition"]
      },
      { // Go to References
        "before": ["<leader>", "v", "r"],
        "commands": ["editor.action.goToReferences"]
      },
      {
        "before": ["<leader>", "v", "h"],
        "commands": ["editor.action.showHover"]
      },
      { // Show Editor Context Menu
        "before": ["<leader>", "v", "c"],
        "commands": ["editor.action.showContextMenu"]
      },
      { // Project files 
        "before": ["<leader>", "p", "f"],
        "commands": ["find-it-faster.findFiles"]
      },
      { // Project files with filter 
        "before": ["<leader>", "p", "f", "f"],
        "commands": ["find-it-faster.findFilesWithType"]
      },
      { // Project search 
        "before": ["<leader>", "p", "s"],
        "commands": ["find-it-faster.findWithinFiles"]
      },
      { // Project search with fitler 
        "before": ["<leader>", "p", "s", "f"],
        "commands": ["find-it-faster.findWithinFilesWithType"]
      },
      {
        "before": ["<leader>", "e"],
        "commands": ["workbench.explorer.fileView.focus"]
      },
      {
        "before": ["<leader>", "q"],
        "commands": ["workbench.action.closeActiveEditor"]
      },
      {
        "before": ["<leader>", "u", "p"],
        "commands": ["extension.ftpsyncuploadselected"]
      },
      {
        "before": ["<leader>", "s", "o"],
        "commands": ["sessions.open"]
      },
      { // Keep cursor in the middle when ctrl + d
          "before": ["<C-d>"],
          "after": [ "<C-d>", "z", "z"]
      },
      { // Keep cursor in the middle when ctrl + u
          "before": ["<C-u>"],
          "after": [ "<C-u>", "z", "z"]
      },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
      {
          "before": ["<leader>", "d"],
          "after": [ "\"", "_", "d" ]
      },
      {
          "before": ["<leader>", "p"],
          "after": [ "\"", "_", "d", "P" ]
      },
    ],
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false,
      "<C-b>": false,
      "<C-n>": false,
      "<C-l>": true,
      "<C-h>": true,
      "space e": false,
      "space ps": true
    },
    // "vscode_custom_css.imports": [
    //   "file:///C:/Users/Administrator/AppData/Roaming/Code/User/custom.css",
    //   "file:///C:/Users/Administrator/AppData/Roaming/Code/User/custom.js"
    // ],
    "vim.leader": "<space>",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.activityBar.visible": false,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "editor.cursorBlinking": "solid",
    "window.title": "${folderName}/${activeEditorShort}",
    "workbench.editor.enablePreview": false,
    "editor.scrollBeyondLastLine": false,
    "editor.fontSize": 19,
    "editor.fontWeight": "500",
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": false,
    "nerdtree.hideSidebarWhenOpenFile": true,
    "nerdtree.alwaysShowSidebar": false,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "glassit.alpha": 245,
    "window.menuBarVisibility": "toggle",
    "editor.cursorStyle": "block",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
    "editor.lineNumbers": "relative",
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0.50,
    "editor.cursorSurroundingLines": 8 // Keep your cursor centered
}